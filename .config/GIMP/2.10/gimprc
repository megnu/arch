# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(default-image
    (width 1920)
    (height 1080)
    (unit pixels)
    (xresolution 300)
    (yresolution 300)
    (resolution-unit inches)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type background)
    (comment ""))
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(check-update-timestamp 1585295655)
(last-release-timestamp 1582416000)
(last-known-release "2.10.18")
(monitor-xresolution 125)
(monitor-yresolution 127)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary no)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid yes)
    (snap-to-canvas yes)
    (snap-to-path yes)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary no)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid yes)
    (snap-to-canvas yes)
    (snap-to-path yes)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(move-tool-changes-active yes)
(toolbox-color-area no)
(toolbox-wilber no)
(toolbox-groups no)
(theme "System")
(icon-theme "Color")
(color-profile-policy keep)
(color-profile-path "${gimp_installation_dir}/share/color/icc/colord")
(image-convert-indexed-max-colors 3)
(image-convert-indexed-remove-duplicates no)
(image-resize-fill-type white)
(layer-new-name "fd")
(layer-resize-fill-type foreground)
(selection-feather-radius 6)
(selection-feather-edge-lock no)
(selection-shrink-radius 6)
(selection-border-radius 3)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias no)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
