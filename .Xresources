! ------------------------------------------------------------------------------
! xterm ohia theme
! ------------------------------------------------------------------------------
xterm*faceName: IBMPlexMono
xterm*loginShell: true
xterm*vt100*geometry: 90x34
xterm*faceSize: 12
xterm*allowBoldFonts: false
xterm*assumeAllChars: true
xterm*wideChars: true
xterm*showMissingGlyphs: true
xterm*selectToClipboard: true
xterm*dynamicColors: true
xterm*cursorBlink: false
xterm*saveLines: 2000
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName: xterm-color
xterm*eightBitInput: false
xterm*foreground: #FFFFFF
xterm*background: #000000
! colors
!black dark/light
xterm*color0:    #000000
xterm*color8:    #FCE0E9
!red dark/light
xterm*color1:    #430C12
xterm*color9:    #D27389
!green dark/light
xterm*color2:    #568C2D
xterm*color10:   #73D283
!yellow dark/light
xterm*color3:    #D19573
xterm*color11:   #D2CF73
!blue dark/light
xterm*color4:    #2D308C
xterm*color12:   #73AFD2
!magenta dark/light
xterm*color5:    #8C2D7C
xterm*color13:   #A973D2
!cyan dark/light
xterm*color6:    #2D8C76
xterm*color14:   #73D2BC
!white dark/light
xterm*color7:    #D2E6E2
xterm*color15:   #FFFFFF
xterm*boldMode:         false
xterm*.cursorColor:     #73D275
! ------------------------------------------------------------------------------
! URxvt ohia theme - Reload with xrdb -merge ~/.Xresources
! ------------------------------------------------------------------------------
! general settings
URxvt*saveLines:         20000
URxvt*termName:         xterm-256color
URxvt*urgentOnBell:     false
URxvt*visualBell:       false
URxvt*iso14755:         false
URxvt*iso14755_52:      false
URxvt*meta8:            false
URxvt*metaSendsEscape:  true
! appearance
URxvt*depth:             32
URxvt*transparent:       false
URxvt*shading:           0
! URxvt*background:        rgba:0000/0000/0000/DE00
URxvt*background:        #000000
URxvt*foreground:        #FFFFFF
URxvt*underlineColor:    #73D2BC
URxvt*highlightColor:    #73D2BC
URxvt*throughColor:      #D2E6E2
URxvt*cursorColor:       #D2E6E2
URxvt*scrollBar:         false
URxvt*scrollTtyOutput:   true
URxvt*scrollWithBuffer:  true
URxvt*scrollTtyKeypress: true
URxvt.secondaryScreen:   1
URxvt.secondaryScroll:   0
URxvt.secondaryWheel:    1
URxvt*internalBorder:    0
URxvt*externalBorder:    0
URxvt*colorIT:      	 #87af5f
URxvt*colorBD:      	 #c5c8c6
URxvt*colorUL:      	 #D27389
! colors
!black dark/light
URxvt*color0:    #000000
URxvt*color8:    #FCE0E9
!red dark/light
URxvt*color1:    #8C502D
URxvt*color9:    #D27389
!green dark/light
URxvt*color2:    #568C2D
URxvt*color10:   #73D283
!yellow dark/light
URxvt*color3:    #D19573
URxvt*color11:   #D2CF73
!blue dark/light
URxvt*color4:    #2D308C
URxvt*color12:   #73AFD2
!magenta dark/light
URxvt*color5:    #8C2D7C
URxvt*color13:   #A973D2
!cyan dark/light
URxvt*color6:    #2D8C76
URxvt*color14:   #73D2BC
!white dark/light
URxvt*color7:    #D2E6E2
URxvt*color15:   #FFFFFF
! fonts
URxvt*letterSpace:      -1
URxvt*font:             xft:IBMPlexMono:regular:size=12,Noto Color Emoji:size=10
URxvt*imFont:           xft:IBMPlexMono:regular:size=12
URxvt*boldFont:         xft:IBMPlexMono:bold:size=12
URxvt*italicFont:       xft:IBMPlexMono:italic:size=12
URxvt*boldItalicFont:   xft:IBMPlexMono:regular:size=12
! run :.!fc-list | grep <font-name>
! perl extensions
URxvt*perl-ext-common:  default,clipboard,url-select,keyboard-select,resize-font
! macros for clipboard and selection
URxvt*copyCommand:      xclip -i -selection clipboard
URxvt*pasteCommand:     xclip -o -selection clipboard
URxvt*keysym.M-c:       perl:clipboard:copy
URxvt*keysym.m-v:       perl:clipboard:paste
URxvt*keysym.M-C-v:     perl:clipboard:paste_escaped
URxvt*keysym.M-Escape:  perl:keyboard-select:activate
URxvt*keysym.M-s:       perl:keyboard-select:search
URxvt*keysym.M-u:       perl:url-select:select_next
URxvt*resize-font.smaller:	C-minus
URxvt*resize-font.bigger:	C-equal
URxvt*urlLauncher:      firefox
URxvt*underlineURLs:    true
URxvt*urlButton:        1
URxvt*geometry: 		90x30
! tabbed
URxvt*tabbed.tabbar-fg: 4
URxvt*tabbed.tabbar-bg: 16
URxvt*tabbed.tab-fg:    15
URxvt*tabbed.tab-bg:    4
URxvt*buffered:         false
URxvt*keysym.Control-Shift-C: perl:clipboard:copy
URxvt*keysym.Control-Shift-V: perl:clipboard:paste
